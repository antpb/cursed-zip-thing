{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "plugin.source.schema.json",
	"title": "WordPress Plugin Source Schema",
	"type": "object",
	"properties": {
	  "name": {
		"type": "string"
	  },
	  "url": {
		"type": "string",
		"format": "uri"
	  }
	},
	"required": ["name", "url"],
	"additionalProperties": false
  }
  